    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Большая статья</title>
    <style>
        body {
            font-family: 'Arial', sans-serif;
            text-align: center;
            background-color: #f0f0f0;
            margin: 0;
            padding: 20px;
            overflow-x: hidden;
        }
		.hidden-object {
    cursor: pointer;
    position: absolute;
    transition: transform 0.2s;
    filter: drop-shadow(0 0 5px rgba(0, 0, 0, 0.3));
    width: 50px;
    height: 50px;
}

.hidden-object img {
    width: 100%;
    height: 100%;
    object-fit: contain;
}

       
        
        .hidden-object:hover {
            transform: scale(1.2);
        }
        
        .particle {
            position: absolute;
            border-radius: 50%;
            pointer-events: none;
            transform: translate(-50%, -50%);
            opacity: 1;
        }
        
        .message {
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            background: linear-gradient(135deg, #ff6b6b, #ffa3a3);
            color: white;
            padding: 20px 40px;
            border-radius: 15px;
            font-size: 24px;
            font-weight: bold;
            z-index: 1000;
            display: none;
            animation: fadeIn 0.5s;
            box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);
            text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.3);
        }
        
        .final-message {
            background: linear-gradient(135deg, #4CAF50, #8BC34A) !important;
            text-align: center;
            max-width: 80%;
        }
        
        .emoji-buttons {
            display: flex;
            justify-content: center;
            gap: 20px;
            margin-top: 20px;
        }
        
        .emoji-btn {
            font-size: 40px;
            cursor: pointer;
            transition: transform 0.2s;
            background: none;
            border: none;
            padding: 0;
        }
        
        .emoji-btn:hover {
            transform: scale(1.2);
        }
        
        @keyframes fadeIn {
            from { opacity: 0; transform: translate(-50%, -60%); }
            to { opacity: 1; transform: translate(-50%, -50%); }
        }
        
        .confetti {
            position: fixed;
            width: 10px;
            height: 10px;
            opacity: 0;
            z-index: 99;
            transform: translate(-50%, -50%);
        }
    </style>
</head>
