    <div class="message" id="message"></div>
    
    <script>
function preloadImages() {
    images.forEach(src => {
        const img = new Image();
        img.src = src;
    });
}

window.onload = function() {
    preloadImages();
    createHiddenObjects();
};

// Конфигурация
const TOTAL_OBJECTS = 3;
let foundObjects = 0;
let finalMessageShown = false;
        
        const images = [
    'game1.webp',
    'game2.webp',
    'game3.webp'
];
        
        // Случайные сообщения при победе
        const messages = [
            "Ты сделал это!",
            "Круто!",
            "Хорошая игра!",
            "Неплохо!",
            "Ура! Ты нашёл!"
        ];
       
        
        // Создаём скрытые объекты
        function createHiddenObjects() {
   const documentHeight = Math.max(
        document.body.scrollHeight, 
        document.documentElement.scrollHeight,
        document.body.offsetHeight, 
        document.documentElement.offsetHeight,
        document.body.clientHeight, 
        document.documentElement.clientHeight
    );

    // Создаем копию массива и перемешиваем его
    const shuffledImages = [...images].sort(() => Math.random() - 0.5);

    for (let i = 0; i < TOTAL_OBJECTS; i++) {
        const object = document.createElement("div");
        object.className = "hidden-object";
        
        // Берем уникальное изображение из перемешанного массива
        const imageSrc = shuffledImages[i];
        const posX = 50 + Math.random() * (window.innerWidth - 150);
        
        // Первый объект - в зоне видимости, остальные - ниже
        let posY;
        if (i === 0) {
            // Первый объект - в видимой области (первые 90% высоты экрана)
            posY = 50 + Math.random() * (window.innerHeight * 0.9 - 100);
        } else {
            // Остальные объекты - ниже видимой области (ниже window.innerHeight)
            const minY = window.innerHeight + 50;
            const maxY = Math.max(documentHeight - 150, minY + 100); // На случай, если документ короткий
            posY = minY + Math.random() * (maxY - minY);
        }
        
        // Создаем элемент img
        object.innerHTML = `<img src="${imageSrc}" width="50" height="50">`;
        object.style.left = `${posX}px`;
        object.style.top = `${posY}px`;
        
        object.addEventListener("click", function(e) {
            explodeObject(e.target);
            createConfetti(e.target);
            showMessage();
            e.target.remove();
            foundObjects++;
            
            if (foundObjects === TOTAL_OBJECTS && !finalMessageShown) {
                finalMessageShown = true;
                setTimeout(() => {
                    showFinalMessage();
                }, 2700);
            }
        });
        
        document.body.appendChild(object);
    }
}

        // Эффект взрыва с медленными частицами
        function explodeObject(element) {
		new Audio("boom.mp3").play();
            const rect = element.getBoundingClientRect();
            const centerX = rect.left + rect.width / 2;
            const centerY = rect.top + rect.height / 2;
            const color = element.style.color;
            
            // Создаём 25 частиц
            for (let i = 0; i < 5; i++) {
                const particle = document.createElement("div");
                particle.className = "particle";
                particle.style.backgroundColor = color;
                particle.style.left = `${centerX}px`;
                particle.style.top = `${centerY}px`;
                particle.style.width = "8px";
                particle.style.height = "8px";
                
                // Случайное направление и скорость
                const angle = Math.random() * Math.PI * 2;
                const speed = 0.5 + Math.random() * 1.5;
                const distance = 50 + Math.random() * 150;
                const size = 5 + Math.random() * 10;
                
                document.body.appendChild(particle);
                
                // Анимация разлёта
                const startTime = Date.now();
                const duration = 3500 + Math.random() * 1000;
                
                const animate = () => {
                    const elapsed = Date.now() - startTime;
                    const progress = Math.min(elapsed / duration, 1);
                    
                    if (progress >= 1) {
                        particle.remove();
                        return;
                    }
                    
                    const currentDistance = progress * distance;
                    particle.style.left = `${centerX + Math.cos(angle) * currentDistance}px`;
                    particle.style.top = `${centerY + Math.sin(angle) * currentDistance}px`;
                    particle.style.opacity = `${1 - progress}`;
                    particle.style.width = `${8 + size * progress}px`;
                    particle.style.height = `${8 + size * progress}px`;
                    
                    requestAnimationFrame(animate);
                };
                
                requestAnimationFrame(animate);
            }
        }
        
        // Конфетти-эффект из центра объекта (без "дёрганий")
        function createConfetti(element) {
            const rect = element.getBoundingClientRect();
            const centerX = rect.left + rect.width / 2;
            const centerY = rect.top + rect.height / 2;
            const confettiColors = ['#f44336', '#e91e63', '#9c27b0', '#673ab7', '#3f51b5', '#2196f3', '#03a9f4', '#00bcd4', '#009688', '#4CAF50', '#8BC34A', '#CDDC39', '#FFEB3B', '#FFC107', '#FF9800', '#FF5722'];
            
            for (let i = 0; i < 50; i++) {
                const confetti = document.createElement("div");
                confetti.className = "confetti";
                confetti.style.left = `${centerX}px`;
                confetti.style.top = `${centerY}px`;
                confetti.style.backgroundColor = confettiColors[Math.floor(Math.random() * confettiColors.length)];
                confetti.style.width = `${10 + Math.random() * 10}px`;
                confetti.style.height = `${10 + Math.random() * 10}px`;
                confetti.style.borderRadius = Math.random() > 0.5 ? "50%" : "0";
                
                document.body.appendChild(confetti);
                
                // Анимация падения (с ограничением границ)
                const angle = Math.random() * Math.PI * 2;
                const velocity = 1 + Math.random() * 3;
                const spin = (Math.random() - 0.5) * 10;
                let spinAngle = 0;
                
                let posX = centerX;
                let posY = centerY;
                let opacity = 1;
                let scale = 1;
                
                const animateConfetti = () => {
                    posY += velocity;
                    posX += Math.sin(angle) * 2;
                    spinAngle += spin;
                    opacity -= 0.002;
                    scale -= 0.001;
                    
                    // Ограничение границ
                    if (posY > window.innerHeight || posX < 0 || posX > window.innerWidth || opacity <= 0) {
                        confetti.remove();
                        return;
                    }
                    
                    confetti.style.top = `${posY}px`;
                    confetti.style.left = `${posX}px`;
                    confetti.style.opacity = opacity;
                    confetti.style.transform = `translate(-50%, -50%) rotate(${spinAngle}deg) scale(${scale})`;
                    
                    requestAnimationFrame(animateConfetti);
                };
                
                setTimeout(() => {
                    requestAnimationFrame(animateConfetti);
                }, Math.random() * 500);
            }
        }
        
        // Показываем сообщение
        function showMessage() {
            const message = document.getElementById("message");
            const remaining = TOTAL_OBJECTS - foundObjects - 1;
            const randomMessage = messages[Math.floor(Math.random() * messages.length)];
			if (remaining == 2) {
			message.textContent = `Игра началась! Найди на странице остальные две игровых иконки и получи подарок. Осталось ${remaining} ${getNoun(remaining, ['иконка', 'иконки', 'иконок'])}.`;
			} else {
			message.textContent = `${randomMessage} Осталось ${remaining} ${getNoun(remaining, ['иконка', 'иконки', 'иконок'])}.`;
			}
            
            message.style.display = "block";
            
            setTimeout(() => {
                if (!finalMessageShown) {
                    message.style.display = "none";
                }
            }, 3000);
        }
        
        // Финальное сообщение с кнопками (не закрывается автоматически)
        function showFinalMessage() {
            const message = document.getElementById("message");
            message.innerHTML = `
                <div>Все иконки игр найдены! Как тебе наша геймификация?</div>
                <div class="emoji-buttons">
                    <button class="emoji-btn" id="like-btn">👍</button>
                    <button class="emoji-btn" id="dislike-btn">👎</button>
                </div>
            `;
            message.className = "message final-message";
            message.style.display = "block";
            
            // Добавляем обработчики для кнопок
            document.getElementById('like-btn').addEventListener('click', handleFeedback);
            document.getElementById('dislike-btn').addEventListener('click', handleFeedback);
        }
        
        // Обработка выбора эмодзи
        function handleFeedback() {
            const message = document.getElementById("message");
            message.innerHTML = "Спасибо за участие!";
            message.className = "message final-message";
            
            setTimeout(() => {
                message.style.display = "none";
            }, 2000);
        }
        
        // Склонение существительных
        function getNoun(number, titles) {
            const cases = [2, 0, 1, 1, 1, 2];
            return titles[(number % 100 > 4 && number % 100 < 20) ? 2 : cases[Math.min(number % 10, 5)]];
        }
        
        // Запускаем игру
        window.onload = createHiddenObjects;
    </script>
</body>
